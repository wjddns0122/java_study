package Chap3;

public class NumberTypeDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int mach;
		int distance;
		mach = 340;
		distance = mach * 60 * 60;
		System.out.println(distance);
		
		double radius;
		double area;
		radius = 10.0;
		area = radius * radius * 3.14;
		System.out.println(area);
	}

}
